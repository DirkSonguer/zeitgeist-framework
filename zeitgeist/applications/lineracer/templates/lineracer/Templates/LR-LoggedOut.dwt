		<div class="userwarning"><p><img src="@@{templatepath}@@/images/messagebox_info.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> @@{userwarning}@@</p></div>
	<!-- TemplateEndBlock -->

