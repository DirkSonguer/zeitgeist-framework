		</div>
	<!-- TemplateEndBlock -->
        
	<!-- TemplateBeginBlock name="usermessages" -->
		<div class="usermessage"><p><img src="@@{templatepath}@@/images/ok.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> @@{usermessage}@@</p></div>
	<!-- TemplateEndBlock -->

