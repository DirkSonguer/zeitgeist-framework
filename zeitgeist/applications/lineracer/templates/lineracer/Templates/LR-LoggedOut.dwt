            <label>Passwort</label><input type="text" name="password" class="txtbig" />
			<input type="submit" name="login" value="Login" class="formbutton" />
            <a href="#" class="btn01 right"><span>Login</span></a>
        </form>
		<!-- TemplateEndBlock -->
		
