<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
<!-- TemplateBeginEditable name="doctitle" -->
<title>LineRacer - Startseite</title>
<!-- TemplateEndEditable -->

<script type="text/javascript" src="js/jquery-1.3.2.min.js"></script>
<script type="text/javascript" src="js/ui.core.js"></script>
<script type="text/javascript" src="js/ui.tabs.js"></script>
<link href="css/layout.css" rel="stylesheet" type="text/css" />
<link href="css/typo.css" rel="stylesheet" type="text/css" />


<!-- TemplateParam name="LoggedIn" type="boolean" value="false" --><!-- TemplateParam name="Startseite" type="boolean" value="false" --><!-- TemplateBeginEditable name="head" --><!-- TemplateEndEditable --><!-- TemplateParam name="TabBoxengasse" type="text" value="m1" --><!-- TemplateParam name="TabLobby" type="text" value="m2" --><!-- TemplateParam name="TabStatistikCenter" type="text" value="m3" --><!-- TemplateParam name="TabShop" type="text" value="m4" -->
</head>
<body>
<div id="canvas">
<div id="header">

      <div class="loginarea">

		<!-- TemplateBeginBlock name="loginbox" -->
		<form method="post" action="@@{[main.login]}@@" name="loginform" id="loginform">
        	<label>Username</label><input type="text" name="username" class="txtbig" />
            <label>Passwort</label><input type="text" name="password" class="txtbig" />
			<input type="hidden" name="login" value="Login" />
            <a href="javascript:login()" class="btn01 right"><span>Login</span></a>
        </form>
		<!-- TemplateEndBlock -->
		
		<!-- TemplateBeginBlock name="logoutbox" -->
		<div class="playerdata">
        	<img src="images/dummy-avatar.gif"  alt="Playerbild" class="avatar"  />
        	<b>@@{playername}@@</b><br />
            Punkte: @@{racepoints}@@<br />
            Gesamtwertung: Platz @@{playerposition}@@
        </div>
        
        <div class="options">
        	<a href="@@{[main.profile]}@@" class="btn01 right"><span>Mein Profil</span></a>
            <a href="@@{[main.logout]}@@" class="btn01 right"><span>Abmelden</span></a>
        </div>
		<!-- TemplateEndBlock -->

      </div>

	<div class="usermessagearea">
    
	<!-- TemplateBeginBlock name="lobbybox" -->
		<div class="textbox" style="width:400px;">
			<p><img src="images/messagebox_info.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> Du wartest bereits auf ein Rennen: <a href="@@{[lobby.showgameroom]}@@">zur Rennvorbereitung</a></p>
		</div>
	<!-- TemplateEndBlock -->	
       
	<!-- TemplateBeginBlock name="gamebox" -->
		<div class="textbox" style="width:400px;">
			<p><img src="images/messagebox_info.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> Du fährst gerade ein Rennen: <a href="@@{[game.index]}@@">zum Rennen</a></p>
		</div>
	<!-- TemplateEndBlock -->
        
	<!-- TemplateBeginBlock name="usermessages" -->
		<div class="usermessage"><p><img src="images/ok.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> @@{usermessage}@@</p></div>
	<!-- TemplateEndBlock -->

	<!-- TemplateBeginBlock name="userwarnings" -->
		<div class="userwarning"><p><img src="images/messagebox_info.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> @@{userwarning}@@</p></div>
	<!-- TemplateEndBlock -->

	<!-- TemplateBeginBlock name="usererrors" -->
		<div class="usererror"><p><img src="images/no.png" alt="" align="left" style="margin-right:5px;margin-left:5px;" /> @@{usererror}@@</p></div>
	<!-- TemplateEndBlock -->
	</div>

      <a href="#"><img src="images/logo-lineracer.png" alt="LineRacer - Das Strategierennen" id="logo" /></a>
      
      <ul class="maintabnav">
        <li><a href="@@{[main.index]}@@" class="@@(TabBoxengasse)@@"><b></b><em>Boxengasse</em></a></li>
        <li><a href="@@{[lobby.index]}@@" class="@@(TabLobby)@@"><b></b><em>Lobby</em></a></li>
        <li><a href="@@{[statistics.index]}@@" class="@@(TabStatistikCenter)@@"><b></b><em>Statistik-Center</em></a></li>
        <li><a href="@@{[shop.index]}@@" class="@@(TabShop)@@"><b></b><em>Shop</em></a></li>
      </ul>

